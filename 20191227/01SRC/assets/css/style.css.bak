@font-face {
  font-family: SukhumvitSet-Thin;
  src: url(/assets/fonts/SukhumvitSet-Thin.woff);
}
@font-face {
  font-family: SukhumvitSet-Medium;
  src: url(/assets/fonts/SukhumvitSet-Medium.woff);
}
@font-face {
  font-family: SukhumvitSet-Bold;
  src: url(/assets/fonts/SukhumvitSet-Bold.woff);
}
body {
  width: 100%; 
  margin: 0px; font-family: SukhumvitSet-Medium; 
  -webkit-user-select: none; -moz-user-select: none; 
  -ms-user-select: none; user-select: none; 
  position: relative; 
  z-index: 0;
  }


.main { width: 1000px; min-width: 1000px; height: 768px; margin: 0 auto; position: relative; z-index: 2;}


