#fbPanel_login { width: 270px; height: 50px; border: 1px solid #01223a; background: url("/assets/images/fb_panel.png");
  position: relative; top: -1px; float: left; overflow: hidden;
  border-bottom-left-radius: 15px; border-bottom-right-radius: 15px;
}
#fbPanel_login .user_pic { width: 40px; height: 40px; border-radius: 10px; position: absolute; margin: 2px 0 0 10px; border: 1px solid #145a8c; float: left; background: #000; text-align: center; overflow: hidden;}
#fbPanel_login .user_pic img { width: 100%; height: auto;}
#fbPanel_login .user_pic_fb { width: 21px; height: 21px; background: url("/assets/images/icon_fb.png"); position: absolute; right: -4px; bottom: -4px;  float: left;}
#fbPanel_login .user_fb { width: 100%; height: 25px; float: left;}
#fbPanel_login .user_name { width: 180px; height: 25px; line-height: 25px; margin: 0 0 0 60px; font-size: 16px; color: #fff; float: left; overflow: hidden;}

#fbPanel_login .user_panel { width: 100%; height: 30px; float: left;}
#fbPanel_login .user_credit_icon { width: 30px; height: 29px; background: url("/assets/images/icon_chip.png"); position: absolute; left: 62px; margin: -1px 0 0 0; float: left;}
#fbPanel_login .user_credit { width: 168px; height: 19px; line-height: 19px; font-size: 10px; color: #a5a5a5; margin: 4px 0 0 60px; background: url("/assets/imgaes/fbLogin_creditBG.png") no-repeat; position: relative; float: left;}
#fbPanel_login #credit { min-width: 100px; height: 25px; margin: 0 0 0 35px; font-family: SukhumvitSet-Bold; font-size: 14px; color: #e19105; font-style: bold; float: left;}
#fbPanel_login .bt_refill { width: 18px; height: 18px; background: url("/assets/images/icon_addCredit.png"); position: absolute;
  right: 60px; margin: 4px 0 0 0; float: left; cursor: pointer;}
#fbPanel_login .bt_refill:hover { background: url("/assets/images/icon_addCredit_hover.png");}
#fbPanel_login .bt_refill:active { margin: 6px 0 0 0;}
#fbPanel_login .bt_logout { width: 59px; height: 59px; background: url("/assets/images/icon_logout.png");
 position: absolute; top: -3px; right: 0; float: left; cursor: pointer;}
#fbPanel_login .bt_logout:hover { background: url("/assets/images/icon_logout_hover.png");}
#fbPanel_login .bt_logout:active { }


.table { width: 800px; height: 254px; margin: 10px 0 0 10px; float: left; overflow: hidden;}

.table_body { width: 100%; height: 250px; margin: 0; background: #282828; float: left; overflow: hidden;}
.table_body table { padding: 0px; margin: 0px;}
.table_body table tr { padding: 0px; margin: 0px;}
.table_body table td { width: 25px; height: 25px; background: #111;}
.table_body table td:hover { background: #222;}
.table_body table #table_chips_player { width: 20px; height: 20px; border-radius: 10px; margin: 1px 0 0 2px; background: url("/assets/images/table_chips_player.png");}
.table_body table #table_chips_tie { width: 20px; height: 20px; border-radius: 10px; margin: 1px 0 0 2px; background: url("/assets/images/table_chips_tie.png");}
.table_body table #table_chips_banker { width: 20px; height: 20px; border-radius: 10px; margin: 1px 0 0 2px; background: url("/assets/images/table_chips_banker.png");}


